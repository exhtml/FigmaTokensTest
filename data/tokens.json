{
  "global": {
    "colors": {
      "pink": {
        "value": "#ff1946",
        "type": "color",
        "description": "Use it for highlighting things"
      },
      "orange": {
        "value": "#ffb519",
        "type": "color",
        "description": "Use it for warnings"
      }
    },
    "gradients": {
      "one": {
        "value": "linear-gradient(45deg, rgba($colors.pink, 0.5) 0%, rgba($colors.orange, 1) 100%)",
        "type": "color"
      }
    },
    "sm": {
      "value": "8",
      "type": "spacing"
    },
    "md": {
      "value": "{sm} * 2",
      "type": "spacing"
    },
    "lg": {
      "value": "{sm} * 3",
      "type": "spacing"
    },
    "xl": {
      "value": "{sm} * 4",
      "type": "spacing"
    },
    "background": {
      "primary": {
        "value": "rgba({colors.pink}, 0.5)",
        "type": "color",
        "description": "Background general"
      }
    },
    "fontFamilies": {
      "space-grotesk": {
        "value": "Space Grotesk",
        "type": "fontFamilies"
      },
      "montserrat": {
        "value": "Montserrat",
        "type": "fontFamilies"
      }
    },
    "lineHeights": {
      "0": {
        "value": "36",
        "type": "lineHeights"
      },
      "1": {
        "value": "30",
        "type": "lineHeights"
      },
      "2": {
        "value": "16",
        "type": "lineHeights"
      }
    },
    "fontWeights": {
      "space-grotesk-0": {
        "value": "Bold",
        "type": "fontWeights"
      },
      "space-grotesk-1": {
        "value": "Regular",
        "type": "fontWeights"
      },
      "montserrat-2": {
        "value": "Regular",
        "type": "fontWeights"
      }
    },
    "fontSize": {
      "0": {
        "value": "12",
        "type": "fontSizes"
      },
      "1": {
        "value": "24",
        "type": "fontSizes"
      },
      "2": {
        "value": "32",
        "type": "fontSizes"
      }
    },
    "letterSpacing": {
      "0": {
        "value": "0%",
        "type": "letterSpacing"
      }
    },
    "paragraphSpacing": {
      "0": {
        "value": "0",
        "type": "paragraphSpacing"
      },
      "1": {
        "value": "8",
        "type": "paragraphSpacing"
      }
    },
    "Heading": {
      "H1": {
        "value": {
          "fontFamily": "$fontFamilies.space-grotesk",
          "fontWeight": "$fontWeights.space-grotesk-0",
          "lineHeight": "$lineHeights.0",
          "fontSize": "$fontSize.2",
          "letterSpacing": "$letterSpacing.0",
          "paragraphSpacing": "$paragraphSpacing.0",
          "textCase": "$textCase.uppercase",
          "textDecoration": "$textDecoration.none"
        },
        "type": "typography"
      },
      "H2": {
        "value": {
          "fontFamily": "$fontFamilies.space-grotesk",
          "fontWeight": "$fontWeights.space-grotesk-1",
          "lineHeight": "$lineHeights.1",
          "fontSize": "$fontSize.1",
          "letterSpacing": "$letterSpacing.0",
          "paragraphSpacing": "$paragraphSpacing.0",
          "textCase": "$textCase.uppercase",
          "textDecoration": "$textDecoration.none"
        },
        "type": "typography"
      }
    },
    "Body": {
      "Text": {
        "value": {
          "fontFamily": "$fontFamilies.montserrat",
          "fontWeight": "$fontWeights.montserrat-2",
          "lineHeight": "$lineHeights.2",
          "fontSize": "$fontSize.0",
          "letterSpacing": "$letterSpacing.0",
          "paragraphSpacing": "$paragraphSpacing.1",
          "textCase": "$textCase.none",
          "textDecoration": "$textDecoration.none"
        },
        "type": "typography"
      }
    },
    "textCase": {
      "uppercase": {
        "value": "uppercase",
        "type": "textCase"
      },
      "none": {
        "value": "none",
        "type": "textCase"
      }
    },
    "textDecoration": {
      "none": {
        "value": "none",
        "type": "textDecoration"
      }
    }
  },
  "MySet": {
    "background": {
      "primary": {
        "value": "#ccff7e",
        "type": "color"
      }
    }
  },
  "$themes": []
}